{
  "name": "linkedin_scraper",
  "version": "1.0.0",
  "main": "dist/app.ts",
  "repository": "https://github.com/Millie-Group/linkedin_scraper",
  "author": "judehunter <matisowagm@gmail.com>",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "build": "tsc",
    "migration:generate": "dotenv -e .env.prod -- typeorm-ts-node-esm migration:generate --dataSource ./src/db.ts",
    "migration:run": "typeorm-ts-node-esm migration:run --dataSource ./src/db.ts",
    "migration:rundotenv": "dotenv -e .env.prod -- typeorm-ts-node-esm migration:run --dataSource ./src/db.ts"
  },
  "dependencies": {
    "@koa/cors": "^3.4.1",
    "@koa/router": "^12.0.0",
    "@slack/bolt": "^3.12.1",
    "cancelable-promise": "^4.3.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "nanoid": "3.0.0",
    "pg": "^8.8.0",
    "python-shell": "^3.0.1",
    "slackbots": "^1.3.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.8",
    "@types/node": "^18.7.23",
    "dotenv-cli": "^6.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
